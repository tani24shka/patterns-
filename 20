void getNumberPattern(int n) {
    // Write your code here.
    for(int i=0;i<2*n-1;i++)
    {
        for(int j=0;j<2*n-1;j++)
        {
           int top=i;
           int bottom=2*n-i-2;
           int left=j;
           int right=2*n-j-2;
           cout<<(n-(min(min(top,bottom),min(left,right))));
        }
        cout<<endl;
    }
}
